<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ascend.wangfeng.locationbyhand">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_my_launcher"
        android:label="${app_name}"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.ascend.wxldcmenu.MenuMainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".keeplive.LiveActivity"
            android:launchMode="singleInstance"
            android:theme="@style/LiveStyle" />

        <service android:name=".keeplive.LiveService" />

        <activity android:name="com.ascend.wxldcmenu.MainActivity"></activity>
    </application>

</manifest>